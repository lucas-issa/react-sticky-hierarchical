{
  "name": "react-sticky-stack",
  "version": "0.0.1",
  "description": "A Sticky Stack component for React.js",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src",
    "build": "babel ./src --out-dir ./dist",
    "demo": "webpack ./demo/index.js ./demo/public/bundle.js",
    "start": "node ./demo/server"
  },
  "authors": [
    "Alex Andrews",
    "Ben Davis",
    "Lucas Motta",
    "Michele Bertoli"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.21",
    "babel-eslint": "^4.1.6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.11.3",
    "express": "^4.13.3",
    "jest-cli": "^0.8.2",
    "react-addons-test-utils": "^0.14.3",
    "webpack": "^1.12.9"
  },
  "dependencies": {
    "css-element-queries": "^0.4.0",
    "lodash": "^4.17.4",
    "react": "^0.14.3",
    "react-dom": "^0.14.3"
  },
  "jest": {
    "testPathDirs": [
      "./src"
    ],
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  },
  "tags": [
    "react",
    "sticky",
    "stack"
  ],
  "keywords": [
    "react",
    "react-component",
    "sticky",
    "stack"
  ]
}
